namespace dev.thiagofontes

/**
 * My sensors Network
 */
participant User identified by userId{
  o String userId
  o String userName
}

asset Sensor identified by sensorId {
  o String sensorId
  o String model
  o String encryptionKey
  --> User owner
  --> User[] users
}

transaction givePermission {
  --> Sensor sensor
  --> User user
}

transaction changeOwner {
  --> Sensor sensor
  --> User newOwner
}
